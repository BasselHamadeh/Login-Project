<Application x:Class="TicTacTo.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TicTacTo"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor" Color="DarkOrange"></SolidColorBrush>
        <SolidColorBrush x:Key="ButtonColor" Color="Yellow"></SolidColorBrush>
        <SolidColorBrush x:Key="LineColor" Color="LightGreen"></SolidColorBrush>
    </Application.Resources>
</Application>
